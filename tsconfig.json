{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "noEmit": true,
    "baseUrl": "./",
    "strict": true,
    "paths": {
      "@generated/*": [
        "generated/*"
      ],
      "@lib/*": [
        "lib/*"
      ],
      "@routes/*": [
        "./app/*"
      ],
      "@services/*": [
        "lib/services/*"
      ],
      "@utils/*": [
        "lib/utils/*"
      ],
      "@components/*": [
        "lib/components/*"
      ],
      "~/*": [
        "*"
      ]
    }
  },
  "include": [
    "**/*.ts",
    "**/*.tsx",
    ".expo/types/**/*.ts",
    "expo-env.d.ts",
    "lib/services/db/importCSV.js",
    "api/index.js",
    "lib/useColorScheme.tx",
    "nativewind-env.d.ts"
  ]
}