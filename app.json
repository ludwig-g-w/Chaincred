{
  "expo": {
    "experiments": {
      "typedRoutes": true
    },
    "platforms": [
      "ios",
      "web",
      "android"
    ],
    "scheme": "chaincred",
    "name": "ChainCred",
    "slug": "chaincred",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.ludwigw.chaincred"
    },
    "android": {
      "config": {
        "googleMaps": {
          "apiKey": "AIzaSyDBx5I1YAQiApOZhhq7aAsj2_19HO4hOfM"
        }
      },
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.ludwigw.chaincred",
      "permissions": [
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO"
      ]
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro",
      "output": "server"
    },
    "plugins": [
      [
        "expo-router",
        {
          "origin": "https://gtfol.xyz"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow $(PRODUCT_NAME) to access your camera."
        }
      ],
      "expo-image-picker",
      "expo-secure-store",
      [
        "expo-secure-store",
        {
          "faceIDPermission": "Allow $(PRODUCT_NAME) to access your Face ID biometric data."
        }
      ],
      [
        "expo-build-properties",
        {
          "android": {
            "minSdkVersion": 26
          },
          "ios": {
            // TODO: verify that this doesn't break the app
            "extraPods": [
              {
                "name": "OpenSSL-Universal",
                "configurations": [
                  "Release",
                  "Debug"
                ],
                "modular_headers": true,
                "version": "3.1.5004"
              }
            ]
          }
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "ff56839b-d376-4b6f-bbae-895cf64f49ea"
      }
    },
    "owner": "ludwigw",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/ff56839b-d376-4b6f-bbae-895cf64f49ea"
    }
  }
}